  "description": "Backend for Tic-Tac-Toe game",
  "main": "dist/server.js",
  "scripts": {
    "dev": "npx ts-node src/server.ts",
    "dev:watch": "npx nodemon --exec \"npx ts-node src/server.ts\"",
